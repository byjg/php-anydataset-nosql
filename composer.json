{
  "name": "byjg/anydataset-nosql",
  "description": "Anydataset NoSQL standardizes the access to non-relational databases/repositories and provides a consistent interface for both NoSQL document databases and Key/Value stores.",
  "autoload": {
    "psr-4": {
      "ByJG\\AnyDataset\\NoSql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.3 <8.6",
    "ext-curl": "*",
    "aws/aws-sdk-php": "3.*",
    "byjg/cache-engine": "^6.0",
    "byjg/anydataset": "^6.0",
    "byjg/serializer": "^6.0",
    "byjg/webrequest": "^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "vimeo/psalm": "^5.9|^6.13"
  },
  "suggest": {
    "ext-mongodb": "*",
    "byjg/cache-engine": "^6.0"
  },
  "provide": {
    "byjg/anydataset-implementation": "1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  },
  "license": "MIT"
}
